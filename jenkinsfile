pipeline {
    agent {label 'slave01'}
    stages {
        stage('SonarQube Analysis') {
          withSonarQubeEnv(installationName: 'SonarQube Server', credentialsId: 'SonarqubeToken') {
            sh "mvn clean verify sonar:sonar -Dsonar.projectKey=mongo-app"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
    }
}
